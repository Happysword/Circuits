<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="gtest" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug Win32">
				<Option output="$(SolutionName)/Debug/gtest" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="$(SolutionName)/Debug/gtest/" />
				<Option deps_output="$(SolutionName)/Debug/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D_LIB" />
					<Add option="-O0" />
					<Add option="-Wall" />
				</Compiler>
			</Target>
			<Target title="Release Win32">
				<Option output="$(SolutionName)/Release/gtest" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="$(SolutionName)/Release/gtest/" />
				<Option deps_output="$(SolutionName)/Release/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-DWIN32" />
					<Add option="-DNDEBUG" />
					<Add option="-D_LIB" />
					<Add option="-Wall" />
				</Compiler>
			</Target>
		</Build>
		<Unit filename="../src/gtest-all.cc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
